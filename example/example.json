          "required": {
            "type": "boolean"
          },
          "arrayEnum": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": ["example1", "example2"]
            }
          }
        }
      },
      "FooMergePatch": {
        "type": "object",
        "properties": {
          "count": {
            "type": "integer",
            "format": "int64"
          }
        }
