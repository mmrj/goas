          }
        }
      },
      "Birb": {
        "type": "object",
        "required": ["name"],
        "properties": {
          "name": {
            "type": "string"
          },
          "airspeedVelocity": {
            "type": "number"
          }
        }
      },
      "BsonID": {
        "type": "string"
      },
      "DoubleAlias": {
        "$ref": "#/components/schemas/JsonMap"
      },
      "Environment": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          }
        }
      },
      "FooBody": {
        "type": "object",
