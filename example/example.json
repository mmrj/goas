      },
      "FooPatchOperationSet": {
        "type": "object",
        "properties": {
          "operations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FooPatchOperation"
            }
          }
        }
      },
      "FooResponse": {
        "type": "object",
        "properties": {
          "bsonId": {
            "$ref": "#/components/schemas/BsonID"
          },
          "id": {
            "type": "string"
          },
          "startDate": {
            "type": "string",
            "format": "date-time"
          },
          "endDate": {
            "$ref": "#/components/schemas/UnixMillis"
          },
          "count": {
            "type": "integer",
            "format": "int64",
