          "freeForm": {},
          "jsonMap": {
            "$ref": "#/components/schemas/JsonMap"
          },
          "doubleAlias": {
            "$ref": "#/components/schemas/DoubleAlias"
          },
          "interfaceBlah": {
            "$ref": "#/components/schemas/InterfaceResponse"
          },
          "instruction": {
            "$ref": "#/components/schemas/Instruction"
          },
          "bsonPtr": {
            "example": "blah blah blah",
            "$ref": "#/components/schemas/BsonID"
          },
          "randomBool": {
            "type": "boolean",
            "example": true
          },
          "myEnum": {
            "type": "string",
            "enum": ["value1", "value2"]
          },
          "changeReturn": {
            "$ref": "#/components/schemas/Instruction"
          }
